{
  "name": "andrelandgraf",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "update:remix": "npm i @remix-run/express@2 @remix-run/node@2 @remix-run/react@2 @remix-run/dev@2 @remix-run/eslint-config@2 @remix-run/css-bundle@2",
    "clean": "rimraf ./node_modules .cache ./build ./public/build && npm i",
    "routes": "remix routes",
    "typecheck": "tsc",
    "format": "run-s format:*",
    "format:lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "format:db": "npx prisma format",
    "build": "run-s \"build:*\"",
    "build:remix": "remix build",
    "build:db": "npx prisma generate",
    "update:db": "npx prisma db push",
    "update:articles": "bun prisma/articles.ts",
    "dev": "run-p \"dev:*\"",
    "dev:remix": "remix dev --manual",
    "start": "remix-serve ./build/index.js"
  },
  "dependencies": {
    "@markdoc/markdoc": "^0.3.4",
    "@prisma/client": "^5.5.2",
    "@remix-run/css-bundle": "^2.1.0",
    "@remix-run/express": "^2.1.0",
    "@remix-run/node": "^2.1.0",
    "@remix-run/react": "^2.1.0",
    "@remix-run/serve": "^2.0.0",
    "@vercel/remix": "^2.0.0",
    "clsx": "^2.0.0",
    "isbot": "^3.6.8",
    "js-yaml": "^4.1.0",
    "prism-react-renderer": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.1.0",
    "@remix-run/eslint-config": "^2.1.0",
    "@types/js-yaml": "^4.0.8",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prisma": "^5.5.2",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": "^18.0.0"
  }
}
