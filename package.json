{
  "name": "andrelandgraf",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "typecheck": "tsc",
    "format": "run-s format:*",
    "format:lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "format:db": "npx prisma format",
    "build": "run-s \"build:*\"",
    "build:db": "bunx prisma generate",
    "build:remix": "remix vite:build",
    "sentry:build": "bunx sentry-upload-sourcemaps && rm ./public/**/*.map && rm ./build/**/*.map",
    "update:db": "bunx prisma db push",
    "update:articles": "NODE_ENV=production bun prisma/articles.ts",
    "dev": "NODE_ENV=development TZ=Etc/UTC bun ./server/index.ts",
    "start": "NODE_ENV=production TZ=Etc/UTC bun ./server/index.ts"
  },
  "dependencies": {
    "@markdoc/markdoc": "^0.4.0",
    "@neondatabase/serverless": "^0.6.0",
    "@prisma/adapter-neon": "^5.5.2",
    "@prisma/client": "^5.5.2",
    "@react-three/drei": "^9.90.2",
    "@react-three/fiber": "^8.15.12",
    "@remix-run/express": "^2.11.2",
    "@remix-run/node": "^2.11.2",
    "@remix-run/react": "^2.11.2",
    "@remix-run/server-runtime": "^2.9.2",
    "@resvg/resvg-js": "^2.6.2",
    "@sentry/bun": "^8.26.0",
    "@sentry/remix": "^8.26.0",
    "@sentry/vite-plugin": "^2.22.2",
    "clsx": "^2.0.0",
    "compression": "^1.7.4",
    "express": "^4.19.2",
    "isbot": "^4.1.0",
    "js-yaml": "^4.1.0",
    "morgan": "^1.10.0",
    "posthog-js": "^1.161.3",
    "posthog-node": "^4.2.0",
    "prism-react-renderer": "^2.1.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "satori": "^0.11.1",
    "three": "^0.159.0",
    "tiny-invariant": "^1.3.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.8",
    "@remix-run/dev": "^2.11.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.8",
    "@types/morgan": "^1.9.9",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/three": "^0.165.0",
    "@types/ws": "^8.5.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "prettier": "^2.8.8",
    "prisma": "^5.5.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^5.2.13",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
