{
  "name": "andrelandgraf",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "update:remix": "npm i @remix-run/express@2 @remix-run/node@2 @remix-run/react@2 @remix-run/dev@2 @remix-run/eslint-config@2 @remix-run/css-bundle@2",
    "clean": "rimraf ./node_modules .cache ./build ./public/build && npm i",
    "routes": "remix routes",
    "typecheck": "tsc",
    "format": "run-s format:*",
    "format:lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "format:db": "npx prisma format",
    "build": "run-s \"build:*\"",
    "build:db": "npx prisma generate",
    "build:remix": "remix vite:build",
    "sentry:build": "npx sentry-upload-sourcemaps && rm ./public/**/*.map && rm ./build/**/*.map",
    "update:db": "npx prisma db push",
    "update:articles": "bun prisma/articles.ts",
    "dev": "remix vite:dev"
  },
  "dependencies": {
    "@markdoc/markdoc": "^0.3.4",
    "@neondatabase/serverless": "^0.6.0",
    "@prisma/adapter-neon": "^5.5.2",
    "@prisma/client": "^5.5.2",
    "@react-three/drei": "^9.90.2",
    "@react-three/fiber": "^8.15.12",
    "@remix-run/node": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "@remix-run/server-runtime": "^2.9.2",
    "@sentry/remix": "^8.8.0",
    "@vercel/remix": "^2.9.2-patch.2",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "isbot": "^3.6.8",
    "js-yaml": "^4.1.0",
    "prism-react-renderer": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.159.0",
    "tiny-invariant": "^1.3.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.2",
    "@remix-run/eslint-config": "^2.9.2",
    "@types/js-yaml": "^4.0.8",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/three": "^0.165.0",
    "@types/ws": "^8.5.8",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prisma": "^5.5.2",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^5.2.13",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": "^18.0.0"
  }
}
